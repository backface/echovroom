    "buefy": "^0.8.13",
    "clmtrackr": "^1.1.2",
    "core-js": "^3.6.4",
    "css-loader": "^3.4.2",
    "face-api.js": "^0.22.2",
    "facedetector": "^1.0.1",
    "instal": "0.0.1-security",
